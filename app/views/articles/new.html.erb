<h1>新規投稿フォーム</h1>
<%= form_with model: @post_image, local:true do |f| %>
  <h4>タイトル</h4>
  <%= f.text_field :title %>
  <h4>本文</h4>
  <%= f.text_area :body %>
  <h4>エリア選択</h4>
  <%= f.select :category_id, Article.category_ids.keys, {} %>
  <h4>画像</h4>
  <%= f.attachment_field :image %>




  <%= f.submit '投稿' %>
<% end %>