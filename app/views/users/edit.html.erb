<h2>プロフィール編集</h2>
<%= form_with model:@user, local:true do |f| %>
  <p>Name</p>
  <%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"名前"%>
  <p>自己紹介</p>
  <%= f.text_field :self_introduction, autofocus: true, id:"inputName", placeholder:"自己紹介文"%>
  <%= f.submit "変更を保存" %>
<% end %>